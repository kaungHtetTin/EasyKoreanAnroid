<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:height="2dp"
        android:gravity="center">
        <shape>
            <corners
                android:radius="5dp" />
            <gradient
                android:angle="270"
                android:startColor="#569F9F9F"
                android:centerColor="#11707070"
                android:endColor="#11575757"
                android:centerY="0.2"
                android:type="linear"
                />
        </shape>
    </item>
</layer-list>
